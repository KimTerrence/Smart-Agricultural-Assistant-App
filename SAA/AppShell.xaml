<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SAA.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SAA"
    Shell.FlyoutBehavior="Disabled"
    Title="SAA">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
         Title="Login"
         ContentTemplate="{DataTemplate local:LandingPage}"
         Route="LandingPage" />

    <ShellContent
       Title="Register"
       ContentTemplate="{DataTemplate local:RegisterPage}"
       Route="RegisterPage" />

    <ShellContent
   Title="PestDetailPage "
   ContentTemplate="{DataTemplate local:PestDetailPage }"
   Route="PestDetailPage " />

    <ShellContent
 Title="PestListPage"
 ContentTemplate="{DataTemplate local:PestListPage }"
 Route="PestListPage " />





</Shell>
